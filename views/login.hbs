<div class="table-show">
    <div class="content-wrapper">
        <h1>Login</h1>
        <form class="register-form">
            <label for="input-email" class="label-email">
                <input type="email" id="input-email" class="input-email" name="email" placeholder="email">
            </label>
            <label for="input-password" class="label-password">
                <input type="password" id="input-password" class="input-password" name="password" placeholder="password">
            </label>
            <button class="btn-register" id="btn-login" type="submit">login</button>
        </form>
        <a href="/" class="main">main</a>
    </div>
</div>

<script type="module" src="./js/login.js"></script>